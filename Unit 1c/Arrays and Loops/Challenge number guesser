using System;

public class Program 
{
public static void Main ()
{
	number[] guessedNumbers = {new number(), new number(), new number()};
	Console.WriteLine("Can you guess the number I am thinking of?");
	guessedNumbers[0].answerHigh = "10";
	guessedNumbers[1].answerLow = "1";
	guessedNumbers[2].correctGuess = "5";
	for (int k = 10; k >= 1; k--)
	{
  Console.Write (k+ " ");
}
	Console.WriteLine();
	{
	}
	Console.WriteLine(guessedNumbers[1].answerLow);
	foreach(var answerLow in guessedNumbers){
				Console.WriteLine("too low");
	Console.WriteLine(guessedNumbers[0].answerHigh);
		foreach(var answerHigh in guessedNumbers){
			Console.WriteLine("too high");
			Console.WriteLine(guessedNumbers[2].correctGuess);
	foreach(var correctGuess in guessedNumbers){
		Console.WriteLine("Correct");
	}
	}
}
}

public class number {
	public string answerHigh = "10";
public string answerLow = "1";
	public string correctGuess = "5";
}
}