using System;

public class Program 
{
public static void Main ()
{
	Console.WriteLine("What is your favorite food? Is it...");
	Console.WriteLine();
	string[] foodTypes = {"A Pizza", "B Seafood", "C Pasta", "D Burgers"};
	for (var i = 0; i < foodTypes.Length; i++){
	}
	foreach(var favoriteType in foodTypes) {
		Console.WriteLine(favoriteType);
		Console.WriteLine();
}
	Console.WriteLine("You have chosen " + foodTypes[0]);
}
}